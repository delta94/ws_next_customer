{
    "env": {
        "development": {
            "presets": [
                "next/babel",
            ]
        },
        "production": {
            "presets": [
                "next/babel"
            ]
        },
    },
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "alias": {
                    "@/components": "./components/",
                    "@/assets": "./assets/",
                    "@/pages": "./pages/",
                    "@/store": "./store/",
                    "@/styles": "./styles/",
                    "@/types": "./types/",
                    "@/utils": "./utils/",
                    "@/static": "./static/",
                    "@/translations": "./translations/",
                }
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ]
    ]
}